pipeline {
    agent any
    stages {
        stage('Deploy') {
            steps {
                    git branch: 'master',
            credentialsId: 'sajai2',
            url: 'https://github.com/sajai2/Sample.git'
                timeout(time: 3, unit: 'MINUTES') {
                    retry(5) {
                        sh './test.sh'
                    }
                }
            }
        }
    }
}
